<%= simple_form_for @home do |f| %>

<div class="form-group">

  <%= f.input :name, input_html: { class: "form-control"} %>
  <%= f.association :district %>
  <%= f.association :eventdates %>
  <%= f.input :address, input_html: { class: "form-control"} %>

</div>
<%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submiting..." } %>

<script>
  $("#home_district_id").select2();
  $("#home_eventdate_ids").select2();
</script>
<% end %>
